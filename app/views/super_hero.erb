<h1>"Create a Team and Heroes!"</h1>
<form action="/team" method="POST">
   <label for="team_name">Team Name:</label>
   <input id="team_name" type="text" name="team[name]">
   <br>
   <label for="team_motto">Team Motto:</label>
   <input id="team_motto" type="text" name="team[motto]">
   <br>
   <h2>Hero 1</h2>
   <label for="member1_name">Hero Name:</label>
   <input id="member1_name" type="text" name="params['team']['members'][0]['name']">
   <br>
   <label for="member1_power">Hero Power:</label>
   <input id="member1_power" type="text" name="params['team']['members'][0]['power']">
   <br>
   <label for="member1_biography">Hero Biography:</label>
   <input id="member1_biography" type="text" name="params['team']['members'][0]['biography']">
   
</form>
